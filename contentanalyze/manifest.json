{
  "manifest_version": 3,
  "name": "Duplicate Highlighter",
  "version": "1.0",
  "description": "Highlight duplicate words in text",
  "icons": {
    "128": "icons/icon128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icons/icon128.png"
  },
  "permissions": ["storage"]
}